<application>
  <component name="FileTypeManager" version="14">
    <ignoreFiles list="*.hprof;*.pyc;*.pyo;*.rbc;*~;.DS_Store;.git;.hg;.svn;.tox;CVS;RCS;SCCS;__pycache__;_svn;rcs;" />
    <extensionMap>
      <mapping ext="php" type="PHP" />
      <mapping pattern=".npmrc" type="PLAIN_TEXT" />
      <mapping ext="svg" type="XML" />
      <mapping ext="config" type="XML" />
    </extensionMap>
  </component>
</application>